# creates the FizzBuzz function with one numerical input
FizzBuzz <- function(n){
vect <- c(1:n)
for(i in vect){
if(i%%3==0 & i%%5==0){vect[i]="Fizz Buzz"}
else if(i%%3==0){vect[i]="Fizz"}
else if(i%%5==0){vect[i]="Buzz"}
}
return(vect)
}
